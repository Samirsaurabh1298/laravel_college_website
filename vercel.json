{
  "version": 2,
  "builds": [
    {
      "src": "index.php",
      "use": "@vercel/php"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.php"
    }
  ],
  "env": {
    "APP_ENV": "production",
    "APP_KEY": "your-app-key",
    "DB_HOST": "your-db-host",
    "DB_PORT": "3306",
    "DB_USERNAME": "your-db-username",
    "DB_PASSWORD": "your-db-password",
    "DB_DATABASE": "your-db-name"
  }
}
